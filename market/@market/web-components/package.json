{
  "name": "@market/web-components",
  "version": "7.2.1",
  "description": "Square Market Design System Web Components built with Stencil",
  "main": "dist/index.cjs.js",
  "autopublish": true,
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/market/market.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "yarn run build:stencil",
    "build:stencil": "./script/css-to-scss.js && stencil build --docs && yarn run stencil:docs:copy",
    "build:storybook": "stencil build && yarn run stencil:docs:copy && storybook build",
    "deploy:storybook": "yarn run build:storybook && ./script/deploy-storybook.js",
    "lint": "npm-run-all --continue-on-error --print-label --parallel --aggregate-output lint:css lint:js",
    "lint:cache": "npm-run-all --continue-on-error --print-label --parallel --aggregate-output lint:css:cache lint:js:cache",
    "lint:cache:fix": "npm-run-all --continue-on-error --print-label --parallel --aggregate-output lint:css:cache:fix lint:js:cache:fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:cache": "stylelint --cache \"src/**/*.css\"",
    "lint:css:cache:fix": "yarn lint:css:cache --fix",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:fix": "npm-run-all --continue-on-error --print-label --parallel --aggregate-output lint:css:fix lint:js:fix",
    "lint:js": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:js:cache": "eslint --cache \"src/**/*.{js,ts,tsx}\"",
    "lint:js:cache:fix": "yarn lint:js:cache --fix",
    "lint:js:fix": "yarn lint:js --fix",
    "prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "prettier:cache": "prettier --check --cache \"src/**/*.{js,ts,tsx}\"",
    "prettier:cache:fix": "prettier --write --cache \"src/**/*.{js,ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "start": "concurrently --kill-others --names \" stencil ,storybook,  copy   \" --prefix-colors \"bgMagenta.bold,bgCyan.bold,bgYellow.bold\" 'yarn run start:stencil' 'yarn run start:storybook' 'yarn run start:stencil:docs:copy'",
    "start:stencil": "stencil build --docs --watch --serve --no-open",
    "start:stencil:docs:copy": "onchange './tmp/market-custom-elements.json' -- yarn run stencil:docs:copy",
    "start:storybook": "wait-on ./dist/market-custom-elements.json ./dist/market-components.json --delay 10000 && storybook dev --port 6006 --quiet",
    "stencil:docs:copy": "cp ./tmp/market-custom-elements.json ./dist/market-custom-elements.json",
    "test": "./script/puppeteer-browser-install.sh && stencil test --spec --e2e",
    "test:integration": "./script/puppeteer-browser-install.sh && stencil test --e2e",
    "test:unit": "stencil test --spec",
    "test:watch": "./script/puppeteer-browser-install.sh && stencil test --spec --e2e --watchAll",
    "tsc": "../../node_modules/typescript/bin/tsc --noEmit"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.10",
    "@market/market-theme": "^31.2.0",
    "@popperjs/core": "2.11.8",
    "@square/qrcode-encoder": "0.0.3",
    "@stencil/core": "^4.18.0",
    "date-fns": "^2.30.0",
    "focus-trap": "^7.5.4",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/credential-providers": "^3.637.0",
    "@aws-sdk/lib-storage": "^3.637.0",
    "@babel/core": "^7.24.6",
    "@babel/helper-get-function-arity": "^7.16.7",
    "@chromatic-com/storybook": "^1.7.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@kurbar/storybook-addon-docs-stencil": "^8.0.0",
    "@squareup/webpack-plugin-build-time-reporter": "^0.1.3",
    "@stencil-community/eslint-plugin": "^0.5.0",
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/react-output-target": "0.5.3",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.2.9",
    "@storybook/web-components": "^8.2.9",
    "@storybook/web-components-webpack5": "^8.2.9",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "26.0.24",
    "@types/lodash-es": "^4.17.12",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^9.1.3",
    "chromatic": "^10.9.6",
    "concurrently": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-square": "^19.1.0",
    "git-rev-sync": "^3.0.2",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "lit": "^3.1.4",
    "lit-html": "^2.8.0",
    "mime": "^3.0.0",
    "modify-source-webpack-plugin": "^4.1.0",
    "mutation-observer": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.38",
    "postcss-extend": "1.0.5",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.8.8",
    "pretty": "^2.0.0",
    "puppeteer": "^21.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "remark-gfm": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "semver-compare": "^1.0.0",
    "stencil-inline-svg": "^1.1.0",
    "storybook": "^8.2.9",
    "storybook-dark-mode": "4.0.1",
    "storybook-design-token": "^3.1.0",
    "storybook-multilevel-sort": "^2.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^4.9.5",
    "wait-on": "^7.2.0",
    "webpack": "^5.91.0"
  }
}